<div id="signup-body">
    <div id="signup-box">
        <div id="signup-box-title">
            <p>Sign Up</p>
        </div>

        <div id="signup-box-form">
            <input class="form-edit" type="text" placeholder="Email" autocomplete="false"/>

            <input class="form-edit" type="text" placeholder="Username" autocomplete="false"/>

            <input class="form-edit" type="password" placeholder="Password" autocomplete="false"/>

            <input class="form-edit" type="password" placeholder="Confirm Password" autocomplete="false"/>

            <input id="signup-box-form-submit-button" type="button" value="Sign Up" />


            <div id="signup-box-form-login">
                <p>Already have an account? </p>
                <input id="signup-box-form-login-button" type="button" value="Log In" />
            </div>
        </div>
    </div>
</div>

<script src="/js/Signup.js"></script>